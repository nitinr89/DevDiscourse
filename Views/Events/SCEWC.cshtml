@{
    string domainUrl = "https://www.devdiscourse.com";
    ViewBag.Title = "Smart City Expo World Congress | Devdiscourse";
    ViewBag.webEdition = ViewBag.region;
    ViewBag.alternateHeadline = "Smart City Expo World Congress";
    ViewBag.pageType = "webpage";
    string imageUrl = "/images/mediapartners/scewcbg";
    ViewBag.PageImage = string.Concat(domainUrl, imageUrl);
    ViewBag.PostUrl = "https://www.devdiscourse.com/events/SCEWC";
    ViewBag.pageKeywords = "Smart City Expo World Congress ,EXPLORE THE CONGRESS, 19-21 NOV 2019 - BARCELONA | GRAN VIA VENUE";
    ViewBag.PageDescription = "The Smart City Expo World Congress is such a gathering of cities,governments and corporate partners.";

}
<style>
    .renderBody {
        margin: 0;
    }

    .fg-color {
        color: #133100;
    }

    .bg-color {
        background-color: #133100;
    }

    .bg-secondary {
        background-color: #2e9e02;
    }

    .fg-secondary {
        color: #2e9e02;
    }

    .why-abu {
        color: #133100;
        font-size: 40px;
        font-weight: 500;
    }


    .banner-content-container {
        padding: 100px 50px 100px 100px;
    }

    .gard {
        background-color: #0c0809;
        background-image: url('/images/mediapartners/scewcbg.png');
        background-size: cover;
        background-repeat: no-repeat;
        background-position: top center;
        position: relative;
    }

    .gard-overlay {
        position: absolute;
        width: 100%;
        height: 100%;
        background-color: rgba(0,0,0,.5);
    }

    .f-40 {
        font-size: 50px;
        font-weight: 600;
    }

    .font-sans-serif {
        font-family: sans-serif;
        color: black;
    }

    .f-20 {
        font-size: 20px;
    }

    .logo-container {
        text-align: right;
    }

    .news-container {
        background-color: #2e9e02;
        margin-top: 30px;
        margin-bottom: 20px;
        padding-bottom: 30px;
        color: #fff;
    }

    .seperator {
        margin: 30px auto;
        width: 70%;
        height: 7px;
        background-color: #ffe926;
    }

    .mid-container {
        background-color: #161615;
        color: #fff;
        box-shadow: 0 -3px 6px 0 rgba(0, 0, 0, 0.34);
    }

    .btn-css {
        border : 1px solid white;
        color: white;
        margin-top: 40px;
        font-size: 15px;
        font-weight: 700;
        padding: 10px 30px;
        font-family: sans-serif;
        border-radius: 7px;
        box-shadow: 0 15px 30px 0 rgba(8, 31, 62, 0.25);
    }

    .btn2-css {
        border: 2px solid #27B6CD;
        color: #27B6CD;
        margin-top: 40px;
        font-size: 15px;
        font-weight: 700;
        padding: 10px 30px;
        font-family: sans-serif;
        border-radius: 7px;
        box-shadow: 0 15px 30px 0 rgba(8, 31, 62, 0.25);
    }
    
    .btn-css:active {
        background-color: #eeda2b;
        color: #133100;
    }

    btn-css:visited {
        color: #133100;
    }

    .btn-css:hover {
        background-color: #711735;
        color: #fff;
    }

    nav.navscrollmenubar {
        overflow-x: auto;
        overflow-y: hidden;
        white-space: nowrap;
        z-index: 500;
        text-align: center
    }

        nav.navscrollmenubar .item {
            display: inline-block
        }

        nav.navscrollmenubar .dropdown {
            float: right
        }

    .item-right {
        float: right
    }

    nav.navscrollmenubar .linkBtn {
        display: inline-block;
        text-align: center;
        padding: 14px 17px;
        text-decoration: none;
        text-transform: uppercase;
        color: #fff;
        font-size: 18px;
        font-family: 'Oswald',sans-serif;
    }

    .event-news {
        height: 240px;
        background-repeat: no-repeat;
        background-size: cover
    }

    .eventDt {
        font-size: 40px;
        color: #fff;
        font-weight: bold;
        font-family:'Oswald',sans-serif;
        font-weight: 900;
    }

    .eventVenue {
        font-size: 20px;
        color: #fff;
    }

    .eventcard-title {
        height: 90px;
        color: #222;
        background-color: #fff;
        font-stretch: condensed;
        font-size: 14px;
        padding: 10px;
    }

    .loadMoreButton {
        font-size: 16px;
        color: #222;
        background-color: #fff;
        display: block;
        line-height: 30px;
        text-align: center;
        cursor: pointer;
        padding: 10px;
        border: 1px solid #eee;
    }

    .event-container {
        padding-top: 180px;
        padding-bottom: 180px;
    }

    @@media (max-width: 2560px) and (min-width: 1500px) {
        .content-container {
            width: 1100px !important;
        }
    }

    @@media(max-width:1440px) {
        .eventcard-title {
            height: 100px;
        }

        .banner-content-container {
            padding: 70px 50px;
        }

        .f-40 {
            font-size: 40px;
        }

        .btn-css {
            font-size: 20px;
        }
    }


    @@media screen and (max-width:1200px) {
        nav.navscrollmenu .dropdown, .item-right {
            float: none
        }
    }

    @@media(max-width:1024px) {
        .eventcard-title {
            height: 110px;
        }
    }

    @@media screen and (max-width:768px) {
        .bg-container {
            background-size: contain;
            background-repeat: no-repeat;
            background-position: top;
        }

        .f-40 {
            font-size: 50px;
        }

        .logo-container {
            text-align: center;
        }

        nav.navscrollmenu {
            padding-left: 35px
        }

        .eventcard-title {
            height: 120px;
        }

        h2 {
            font-size: 22px;
        }
    }

    @@media screen and (max-width:425px) {
        .banner-content-container {
            padding: 40px 20px;
        }

        .why-abu {
            margin-top: 30px;
        }

        .btn-css {
            margin-top: 20px;
            font-size: 18px;
            padding: 10px 20px;
        }
    }
</style>

