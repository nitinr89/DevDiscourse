                                case "Poisonous":
                                case "Damaged roads":
                                case "Deteriorated":
                                case "Substantially increased":
                                    bg = "bg5";
                                    break;
                                case "Not available":
                                case "Not at all":
                                case "Very much deteriorated":
                                    bg = "bg6";
                                    break;
                                default:
                                    bg = "no-answer";
                                    break;
                            }
                            return bg;
                        }
                    }
                });
                photoLayer.add(photos).addTo(map);
                if (markerPage == 1) {
                    map.fitBounds(photoLayer.getBounds());
                }
                markerPage = markerPage + 1;
                if (_clickId == "All") {
                    dataUrl = "https://ideame.io/Survey/GetSResponse?page=" + markerPage;
                } else {
                    dataUrl = "https://ideame.io/Survey/GetSResponse?list=" + idArray.join(',') + "&?page=" + markerPage;
                }
                if (allowGetmarker == true) {
                    PhotoLayer();
                }
            });
        }
