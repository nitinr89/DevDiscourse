@{
    string domainUrl = "https://www.devdiscourse.com";
    ViewBag.Title = "TOGC 2020 | Devdiscourse";
    ViewBag.webEdition = ViewBag.region;
    ViewBag.alternateHeadline = "Transportation oil and gas Congress";
    ViewBag.pageType = "webpage";
    string imageUrl = "/images/mediapartners/togcbg.jpg";
    ViewBag.PageImage = string.Concat(domainUrl, imageUrl);
    ViewBag.PostUrl = "https://www.devdiscourse.com/events/TOGC";
    ViewBag.pageKeywords = "RANSPORTATION OIL AND GAS CONGRESS 2020, SMARTER NETWORKING. SMARTER PIPELINES,TOGC 2020";
    ViewBag.PageDescription = "OGC 2020 is the annual congress dedicated to oil and gas transportation via pipelines including the questions of pipelines automation and digitalization, asset integrity, pipeline construction in challenging conditions, predictive analysis, the cost-effectiveness of the projects and many others.";

}
