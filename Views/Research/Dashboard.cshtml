    <script type="text/javascript">

    var sectors = '@(ViewBag.sector)';
    var loc = '@(ViewBag.location)';
    var sectorArr = [];
    var totalFeedbacks = 0;

    var _resId = "";
    var _repText = "";
    var _repQues = "";
    var markerPage = 1;
