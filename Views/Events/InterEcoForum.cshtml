@{
    string domainUrl = "https://www.devdiscourse.com";
    ViewBag.Title = "Interecoforum Organizing Committee  | Devdiscourse";
    ViewBag.webEdition = ViewBag.region;
    ViewBag.alternateHeadline = "IEF Organizing Committee";
    ViewBag.pageType = "webpage";
    string imageUrl = "/images/mediapartners/iefbg.jpg";
    ViewBag.PageImage = string.Concat(domainUrl, imageUrl);
    ViewBag.PostUrl = "https://www.devdiscourse.com/events/InterEcoForum";
    ViewBag.pageKeywords = "IX INTERECOFORUM,Water for Sustainable Development,19th & 20th March 2020, Marbella, International Forum on Sustainable Development in Spain";
    ViewBag.PageDescription = "InterEcoForum is an annual event dedicated to the topic of Sustainable Development, which is traditionally held in Spain, in March.";

}
<style>
