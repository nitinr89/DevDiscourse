@{
    string domainUrl = "https://www.devdiscourse.com";
    ViewBag.Title = "Cybertech | Devdiscourse";
    ViewBag.webEdition = ViewBag.region;
    ViewBag.alternateHeadline = "Cybertech Global";
    ViewBag.pageType = "webpage";
    string imageUrl = "/images/mediapartners/cybertechbg.png";
    ViewBag.PageImage = string.Concat(domainUrl, imageUrl);
    ViewBag.PostUrl = "https://www.devdiscourse.com/events/CybertechGlobal";
    ViewBag.pageKeywords = "28 - 30 JAN 2020 TEL | AVIV CYBERTECH GLOBAL IN TEL AVIV IS GOING TO BE BIG!";
    ViewBag.PageDescription = "From Tel Aviv and Rome, to Tokyo, Singapore, Panama, and more, Cybertech is the cyber industry’s foremost B2B networking platform conducting industry-related events all around the globe.";

}
<style>
    .renderBody {
        margin: 0;
