@{
    string domainUrl = "https://www.devdiscourse.com";
    ViewBag.Title = "LNG Congress | Devdiscourse";
    ViewBag.webEdition = ViewBag.region;
    ViewBag.alternateHeadline = "6th Lng Congress";
    ViewBag.pageType = "webpage";
    string imageUrl = "/images/mediapartners/lngcongressbg.jpg";
    ViewBag.PageImage = string.Concat(domainUrl, imageUrl);
    ViewBag.PostUrl = "https://www.devdiscourse.com/events/LNGCongress";
    ViewBag.pageKeywords = "6TH INTERNATIONAL LNG CONGRESS, 2nd - 3rd of March, 2020 Belgium, Brussels Tangla Hotel Brussels.";
    ViewBag.PageDescription = "International LNG Congress organized by BGS Group is one of the most intent Liquefied Natural Gas events in Europe.";

}
<style>
    .renderBody {
        margin: 0;
        background: #D96F41;
    }

    .fg-color {
        color: #133100;
    }

    .bg-color {
        background-color: #133100;
    }

    .bg-secondary {
        background-color: #2e9e02;
    }

