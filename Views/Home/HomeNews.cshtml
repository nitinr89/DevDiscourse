@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
@{
    string domainUrl = "https://www.devdiscourse.com";
    string imageUrl = "/AdminFiles/Logo/devediscourse_banner.png";
    ViewBag.webEdition = ViewBag.edition;
    var newsImage = imageUrl.IndexOf("devdiscourse.blob.core.windows.net") != -1 ? imageUrl : string.Concat(domainUrl, imageUrl);
    ViewBag.PageImage = newsImage + "?width=920";
    var edition = ViewBag.webEdition as string;
    var isMobile = HttpContextAccessor.HttpContext.Request.Headers["User-Agent"].ToString().Contains("Mobi", StringComparison.OrdinalIgnoreCase);
}
@{
    switch (edition)
    {
        case "Pacific":
            ViewBag.Title = "Pacific Edition: International Development News from Australia & Japan | Devdiscourse";
            ViewBag.alternateHeadline = "Pacific News - Development News from Australia & Japan | Devdiscourse";
            ViewBag.PageDescription = "Devdiscourse - Get Latest Headline From Pacific. International Development News, Breaking News on Politics, Business, Technology, Science, Health, Education and Entertainment.";
            ViewBag.PostUrl = "https://www.devdiscourse.com/pacific";
            ViewBag.pageKeywords = "International Development News, Devdiscourse Pacific, Devdiscourse Australia, SDGs News, Japan News, New Zealand News, Politics News, Business News, Sports News, Technology News";
            break;
        case "South Asia":
            ViewBag.Title = "South Asia News: International Development News, Breaking News, India News";
            ViewBag.alternateHeadline = "Breaking News from South Asia, International Development News, India News | Devdiscourse";
            ViewBag.PageDescription = "International Development News from South Asia. Breaking News, Opinion, Analysis, Business, Technology, Politics, Health, Education and more.";
            ViewBag.PostUrl = "https://www.devdiscourse.com/south-asia";
            ViewBag.pageKeywords = "International Development News, Devdiscourse News, Breaking News, Live News, India News, India Politics News, South Asia news, SDGs News, Pakistan News, Bangladesh News, Sri Lanka News, Bollywood News, Sports News, Technology News";
            break;
        case "East and South East Asia":
            ViewBag.Title = "East & South East Asia Edition: International Development News | Devdiscourse";
            ViewBag.alternateHeadline = "East & South East Asia News - International Development News | Devdiscourse";
            ViewBag.PageDescription = "Devdiscourse: Get Latest Headline from East & South East Asia. International Development News, Breaking News on Politics, Business, Technology, Science, Health, Education and Entertainment.";
            ViewBag.PostUrl = "https://www.devdiscourse.com/south-east-asia";
            ViewBag.pageKeywords = "Devdiscourse News, International Development News, East & South East Asia News, China News, Japan News, Philippines News, Thailand News, Cambodia News, Vietnam News, Politics, Sports, Technology";
            break;
        case "Europe and Central Asia":
            ViewBag.Title = "Europe & Central Asia Edition: International Development News | Devdiscourse";
            ViewBag.alternateHeadline = "Europe & Central Asia News - International Development News | Devdiscourse";
            ViewBag.PageDescription = "Devdiscourse: Get Latest Headline from Europe & Central Asia. International Development News, Breaking news on Politics, Business, Technology, Science, Health, Education and Entertainment.";
            ViewBag.PostUrl = "https://www.devdiscourse.com/europe-central-asia";
            ViewBag.pageKeywords = "Devdiscourse news, International Development News, Europe News, Central Asia News, SDGs News, Russia News, United Kingdom News, Germany News, France News, Politics News, Sports News, Technology News";
            break;
        case "Central Africa":
