@{
    ViewBag.Title = "Dashboard";
    string domainUrl = "http://www.devdiscourse.com";
    string imageUrl = "/AdminFiles/Logo/devediscourse_banner.png";
    ViewBag.PageDescription = "Daily News, Sustainable Development, Economy, Finance, Health News, Research";
    ViewBag.PageImage = string.Concat(domainUrl, imageUrl);
    ViewBag.webEdition = "Global Edition";
    ViewBag.PostUrl = "http://www.devdiscourse.com";
}
<link rel="stylesheet" href="https://unpkg.com/leaflet@1.4.0/dist/leaflet.css"
      integrity="sha512-puBpdR0798OZvTTbP4A8Ix/l+A4dHDD0DGqYW6RQ+9jxkRFclaxxQb/SJAWZfWAkuyeQUytO7+7N4QKrDh+drA=="
      crossorigin="" />
<link href="~/Content/Map/leaflet.fullscreen.css" rel="stylesheet" />
<link href="~/Content/Map/leaflet.zoomhome.css" rel="stylesheet" />
<link href="~/Content/researchmap.css" rel="stylesheet" />
<style>
    body {
        background-color: #e3e3e3;
    }

    .sans {
        font-family: sans-serif;
    }

    .panel-default {
        border: 1px solid #d3d3d3;
        border-radius: 4px;
        -webkit-box-shadow: 0px 5px 20px rgba(0, 0, 0, 0.05);
        box-shadow: 0px 5px 20px rgba(0, 0, 0, 0.05);
    }

        .panel-default .panel-body {
            padding: 1.5rem 1.25rem;
        }

    .panel + .panel {
        margin-top: 20px;
    }

    .fg-black {
        color: #222 !important;
    }

    .text-decoration {
        text-decoration: none !important;
    }

    .image-cover, .video-cover {
        height: 180px;
        overflow: hidden;
    }

        .image-cover img {
            object-fit: cover;
        }

        .video-cover video {
            max-height: 180px;
            width: auto;
            margin: auto;
        }

    .markerloader {
        height: 4px;
        width: 100%;
        position: relative;
        overflow: hidden;
        background-color: #ddd;
    }

        .markerloader:before {
            display: block;
            position: absolute;
            content: "";
            left: -200px;
            width: 200px;
            height: 4px;
            background-color: #ff6b00;
            animation: loading 2s linear infinite;
        }

    @@keyframes loading {
        from {
            left: -200px;
            width: 30%;
        }

        50% {
            width: 30%;
        }

        70% {
            width: 70%;
        }

        80% {
            left: 50%;
        }

        95% {
            left: 120%;
        }

        to {
            left: 100%;
        }
    }
</style>
<div class="m-b-30 container">
    <h1 class="section-title">
        <span style="background-color: #e3e3e3;">Dashboard </span>
    </h1>
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-4">
            <div class="bg-white panel panel-default">
                <div class="panel-heading">
                    <div style="height:150px;background-color:#444;background-size:cover;background-image:url(/images/partner_cover.jpg)"></div>
                    <img class="img-circle center-block img-thumbnail" style="margin-top:-50px;background-color:#444;" src="~/images/SDGs/icons/sdg_samurai.svg" width="100" height="100" />
                </div>
                <div class="panel-body">
                    @if (ViewBag.userInfo != null)
                    {
                        <h3 class="text-center">@ViewBag.userInfo.FirstName @ViewBag.userInfo.LastName</h3>
                        <p class="sans text-muted text-center">@ViewBag.userInfo.Email</p>
                        <p class="sans text-muted text-center">@ViewBag.userInfo.Country</p>
                    }
                </div>
            </div>
        </div>
        <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
            <div class="panel panel-default" style="height:165px;background-color:#444;color:white;">
                <div class="panel-body">
                    <div class="media">
                        <div class="media-left">
                            <img src="~/images/sectors_all.svg" width="70" style="margin:10px 0 10px 20px" />
                        </div>
                        <div class="media-body">
                            <h3 class="sans media-heading text-right">Sectors</h3>
                            <div class="pull-right" style="font-size:56px;">
                                @ViewBag.sectorcount
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default bg-white" style="height:165px;color:#555;">
                <div class="panel-body">
                    <div class="media">
                        <div class="media-left">
                            <img src="~/images/feedback_count.svg" width="70" style="margin:10px 0 10px 20px" />
                        </div>
                        <div class="media-body">
                            <h3 class="sans media-heading text-right">Feedback Count</h3>
                            <div class="pull-right" id="totalFb" style="font-size:56px;"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-4">
            <div class="bg-white panel panel-default" style="height:350px" id="summaryChart"></div>
        </div>
    </div>
    <div class="m-t-20 row">
        <div class="col-xs-12 col-sm-6">
            <div class="panel panel-default" style="height:162px;background-color:#ff6a00;color:white;">
                <div class="panel-body">
                    <div class="media">
                        <div class="media-left">
                            <img src="~/images/list_view.svg" width="100" />
                        </div>
                        <div class="media-body">
                            <div class="media-heading sans h2">Feedbacks</div>
                        </div>
                    </div>
                </div>
                <div class="bg-white fg-black clearfix panel-body">
                    <a href="@Url.Action("Feedbacks","Research")" class="fg-black text-decoration">
                        <span class="pull-right">Expand →</span>
                    </a>
                </div>
            </div>
        </div>
        <div class="col-xs-12 col-sm-6">
            <div class="panel panel-default" style="height:162px;background-color:#444;color:white;">
                <div class="panel-body">
                    <div class="media">
                        <div class="media-left">
                            <img src="~/images/chart_view.svg" width="100" />
                        </div>
                        <div class="media-body">
                            <div class="media-heading sans h2">Summary Report</div>
                        </div>
                    </div>
                </div>
                <div class="bg-white fg-black clearfix panel-body">
                    <a href="@Url.Action("SummaryReport","Research")" class="fg-black text-decoration">
                        <span class="pull-right">Expand →</span>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>
