    ViewBag.webEdition = ViewBag.region;
    ViewBag.PostUrl = "https://www.devdiscourse.com/events/diversity-in-energy-summit";
    ViewBag.pageKeywords = "Diversity In Energy Summit 2019, Diversity In Energy, Energy Summit, Energy Summit 2019";
