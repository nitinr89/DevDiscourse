            ViewBag.PageDescription = "Devdiscourse: Get Latest Headline from Central Africa. International Development News, Breaking News on Politics, Business, Technology, Science, Health, Education and Entertainment.";
            ViewBag.PostUrl = "https://www.devdiscourse.com/central-africa";
            ViewBag.pageKeywords = "Devdiscourse news, International Development News, Central Africa News, Africa News, SDGs News, Cameroon News, Central African Republic News, Chad News, Congo News, Madagascar News Africa Politics, Funding in East Africa, East Africa Sports News, Technology News";
            break;
        case "East Africa":
            ViewBag.Title = "East Africa Edition: International Development News | Devdiscourse";
            ViewBag.alternateHeadline = "East Africa News - International Development News | Devdiscourse";
            ViewBag.PageDescription = "Devdiscourse: Get Latest Headline From East Africa. International Development News, Breaking News on Politics, Business, Technology, Science, Health, Education and Entertainment.";
            ViewBag.PostUrl = "https://www.devdiscourse.com/east-africa";
            ViewBag.pageKeywords = "Devdiscourse news, International Development News, East Africa News, SDGs News, Kenya News, Rawanda News, Ethiopia News, Tanzania News, Africa Politics, Funding in East Africa, East Africa Sports News, Technology News";
            break;
        case "Southern Africa":
            ViewBag.Title = "Southern Africa Edition: International Development News, Zimbabwe, Zambia, South Africa and more | Devdiscourse";
            ViewBag.alternateHeadline = "Southern Africa News - International Development News, Zimbabwe News, Zambia News | Devdiscourse";
            ViewBag.PageDescription = "Devdiscourse: Get Latest Headline from Southern Africa. International Development News, Breaking News on Politics, Business, Technology, Science, Health, Education and Entertainment.";
            ViewBag.PostUrl = "https://www.devdiscourse.com/southern-africa";
            ViewBag.pageKeywords = "Devdiscourse news, International Development News, Live News, South Africa News, Zimbabwe Politics, Devdiscourse South Africa Edition, SDGs News, Zambia News, Angola News, Politics News, Sports News, Technology News";
            break;
        case "West Africa":
            ViewBag.Title = "West Africa Edition: International Development News, Ghana News, Nigeria News | Devdiscourse";
            ViewBag.alternateHeadline = "West Africa News - International Development News, Ghana News, Nigeria News | Devdiscourse";
            ViewBag.PageDescription = "Devdiscourse: Get Latest Headline from West Africa. International Development News, Breaking News on Politics, Business, Technology, Science, Health, Education and Entertainment.";
            ViewBag.PostUrl = "https://www.devdiscourse.com/west-africa";
            ViewBag.pageKeywords = "Devdiscourse News, West Africa News, International Development News, Live News, Nigeria Politics, Devdiscourse West Africa Edition, SDGs News, Nigeria News, Ghana News, Politics News, Sports News, Technology News";
            break;
        case "Middle East and North Africa":
            ViewBag.Title = "Middle East & North Africa News - International Development News | Devdiscourse";
            ViewBag.alternateHeadline = "Middle East & North Africa News - International Development News | Devdiscourse";
            ViewBag.PageDescription = "Devdiscourse:Get Latest Headline From Middle East & North Africa. International Development News, Breaking News on Politics, Business, Technology, Science, Health, Education and Entertainment.";
            ViewBag.PostUrl = "https://www.devdiscourse.com/middle-east-north-africa";
            ViewBag.pageKeywords = "Devdiscourse News, International Development News, North Africa News, SDGs News, Russia News, United Kingdom News, Germany News, France News, Politics News, Sports News, Technology News";
            break;
        case "North America":
            ViewBag.Title = "North America News - International Development News, United States News, Canada News | Devdiscourse";
            ViewBag.alternateHeadline = "North America News - International Development News, United States News, Canada News | Devdiscourse";
            ViewBag.PageDescription = "Devdiscourse - Get Latest Headline From North America. International Development News, Breaking News on Politics, Business, Technology, Science, Health, Education and Entertainment.";
