                    alert("Please enter location of your institution");
                }
                else if (!(/^\w+([\.-]?\w+)*@@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(_semail))) {
                    alert("Please enter your valid email");
                }
                else {
                    adoptTheTool();
                }
            });
            function adoptTheTool() {
                $.ajax({
                    url: '/Research/AdoptTool',
                    type: 'Post',
                    data: { name: _sname, institution: _institution, location: _location, email: _semail, contact: _contact, neededData: _neededData, geoData: _geoData, themeticArea: _themeticArea, message: _message },
                    dataType: 'Json',
                    success: function (data) {
                        alert(data);
                        $('#adopttoolModal').modal('hide');
                        assignNull();
                    },
                    error: function () {
                    },
                });
            }
            // BE the Samurai --------------------------- //
            $('#sdgsamurai').click(function () {
                if (_loginId == "" || _loginId == null) {
                    alert("Please login First");
                }
                else {
                    $('#sdgsModal').modal();
                }
            });
            $('#closesdgsDialog').click(function () {
                $('#sdgsModal').modal('hide');
            });
            $('#joinSDGsBtn').click(function () {
                _sprofession = $('#sprofes').val();
                _sdesc = $('#sdesc').val();
                _nationality = $('#snation').val();
                _gender = $('#sgender option:selected').val();
                if (_sprofession == "") {
                    alert("Please enter your profession");
                }
                else if (_sdesc == "") {
                    alert("Please enter description");
                }
                else if (_nationality == "") {
                    alert("Please enter your nationality");
                }
                else {
                    joinSDGs();
                }
            });
            function joinSDGs() {
                $.ajax({
                    url: '/Research/SDGsJoin',
                    type: 'Post',
                    data: { profession: _sprofession, description: _sdesc, nationality: _nationality, gender: _gender },
                    dataType: 'Json',
                    success: function (data) {
                        showAlert(data);
                        $('#sdgsModal').modal('hide');
                        assignNull();
                    },
                    error: function () {
                    },
                });
            }
            function assignNull() {
                _sName = _sprofession = _sdesc = _semail = _nationality = _institution = _location = _neededData = _geoData = _message = "";
                $('#sname,#sprofes,#sdesc,#semail,#snation,#atname,#atinst,#atinstLoc,#atemail,#atcontact,#atneedData,#atgeoData,#atmessage').val("");
            }

            $("#sdgscarusal").owlCarousel({
                items: 3,
                nav: false,
                margin: 25,
                startPosition: '0',
                autoplay: true,
                autoplayTimeout: 3000,
                autoplayHoverPause: true,
                loop: true,
                responsive: {
                    0: {
                        items: 1
                    },
                    767: {
                        items: 2
                    },
                    1000: {
                        items: 4
                    }
                }
            });
        });
    </script>
}