@{
    string domainUrl = "https://www.devdiscourse.com";
    ViewBag.Title = "The Global Social and Financial Skills Conference | Devdiscourse";
    ViewBag.webEdition = ViewBag.region;
    ViewBag.alternateHeadline = "The Global Social and Financial Skills Conference";
    ViewBag.pageType = "webpage";
    string imageUrl = "/images/mediapartners/globalndfinbg.jpg";
    ViewBag.PageImage = string.Concat(domainUrl, imageUrl);
    ViewBag.PostUrl = "https://www.devdiscourse.com/events/GlobalndFinancialSkills";
    ViewBag.pageKeywords = "The Global Social and Financial Skills Conference, 30 October - 1 November, Amsterdam, The Netherlands";
    ViewBag.PageDescription = "Devdiscourse is proud media partner of The Global Social and Financial Skills Conference 30 October - 1 November, 2019 Amsterdam, The Netherlands.";

}
<style>
    .renderBody {
        margin: 0;
    }

    .fg-color {
        color: #1d2f36;
    }

    .bg-color {
        background-color: #824DA7;
    }

    .bg-secondary {
        background-color: #a7d7ff;
    }

    .fg-secondary {
        color: #a7d7ff;
    }

    .why-abu {
        color: #1d2f36;
        font-size: 28px;
        font-weight: 500;
    }

    .blank-container {
        padding-top: 35%;
    }

    .banner-content-container {
        padding: 100px 50px;
    }

    .bg-container {
        background-image: url('/images/mediapartners/globalndfinbg.jpg');
        background-size: cover;
        background-repeat: no-repeat;
        background-position: -600px top;
    }

    .f-40 {
        font-size: 40px;
        font-weight: 600;
    }

    .f-25 {
        font-size: 25px;
        font-weight: 400;
    }

    .font-sans-serif {
        font-family: sans-serif;
    }

    .f-20 {
        font-size: 20px;
    }

    .news-container {
        background-color: #a7d7ff;
        margin-top: 30px;
        margin-bottom: 20px;
        padding-bottom: 30px;
        color: #fff;
    }

    .mid-container {
        background-color: #35bfea;
        color: #1d2f36;
        box-shadow: 0 -3px 6px 0 rgba(0, 0, 0, 0.34);
    }

    .btn-css {
        background-color: white;
        color: #923992;
        margin-top: 40px;
        font-size: 24px;
        font-weight: 500;
        padding: 10px 30px;
        font-family: sans-serif;
        border-radius: 30px;
        box-shadow: 0 15px 30px 0 rgba(8, 31, 62, 0.25);
    }

        .btn-css:active {
            background-color: #a7d7ff;
            color: #fff;
        }

        .btn-css:hover {
            background-color: #49a7f5;
            color: #fff;
        }

    nav.navscrollmenubar {
        overflow-x: auto;
        overflow-y: hidden;
        white-space: nowrap;
        z-index: 500;
        text-align: center
    }

        nav.navscrollmenubar .item {
            display: inline-block
        }

        nav.navscrollmenubar .dropdown {
            float: right
        }

    .item-right {
        float: right
    }

    nav.navscrollmenubar .linkBtn {
        display: inline-block;
        text-align: center;
        padding: 14px 17px;
        text-decoration: none;
        text-transform: uppercase;
        color: white;
        font-size: 18px;
        font-family: 'Oswald',sans-serif;
        font-weight:bold;
    }

    .event-news {
        height: 240px;
        background-repeat: no-repeat;
        background-size: cover
    }

    .eventcard-title {
        height: 90px;
        color: #222;
        background-color: #fff;
        font-stretch: condensed;
        font-size: 14px;
        padding: 10px;
    }

    .loadMoreButton {
        font-size: 16px;
        color: #222;
        background-color: #fff;
        display: block;
        line-height: 30px;
        text-align: center;
        cursor: pointer;
        padding: 10px;
        border: 1px solid #eee;
    }

    @@media (max-width: 2560px) and (min-width: 1500px) {
        .content-container {
            width: 1100px !important;
        }
    }

    @@media(max-width:1440px) {
        .eventcard-title {
            height: 100px;
        }

        .banner-content-container {
            padding: 70px 50px;
        }

        .f-40 {
            font-size: 32px;
        }

        .f-25 {
            font-size: 21px;
        }

        .f-20 {
            font-size: 18px;
        }

        .f-18 {
            font-size: 16px;
        }

        .btn-css {
            font-size: 20px;
        }
    }


    @@media screen and (max-width:1200px) {
        nav.navscrollmenu .dropdown, .item-right {
            float: none
        }
    }

    @@media(max-width:1024px) {
        .eventcard-title {
            height: 110px;
        }
    }

    @@media screen and (max-width:768px) {
        .bg-container {
            background-size: contain;
            background-repeat: no-repeat;
            background-position: top;
        }

        .text-right.banner-content-container {
            text-align: left;
        }

        nav.navscrollmenu {
            padding-left: 35px
        }

        .eventcard-title {
            height: 120px;
        }

        h2 {
            font-size: 22px;
        }
    }

    @@media screen and (max-width:425px) {
        .banner-content-container {
            padding: 40px 30px;
        }

        .why-abu {
            margin-top: 30px;
        }

        .btn-css {
            margin-top: 20px;
            font-size: 18px;
            padding: 10px 20px;
        }
    }
</style>

