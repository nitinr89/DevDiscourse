@{
    string domainUrl = "https://www.devdiscourse.com";
    ViewBag.Title = "Powergen India 2021 | Devdiscourse";
    ViewBag.webEdition = ViewBag.region;
    ViewBag.alternateHeadline = "Powergen India 2021";
    ViewBag.pageType = "webpage";
    string imageUrl = "/images/mediapartners/powergenbg";
    ViewBag.PageImage = string.Concat(domainUrl, imageUrl);
    ViewBag.PostUrl = "https://www.devdiscourse.com/events/PowerGen";
    ViewBag.pageKeywords = "Powergen India 2021, 27-29 Jan 2021, A5, IECC, PRAGATI MAIDAN NEW DELHI, INDIA";
    ViewBag.PageDescription = "For more than 15 years, POWERGEN India has served as India's premier forum for the power generation industry. Attracting over 8,000 attendees, POWERGEN India covers all forms of power generation, from conventional to renewable energy and other low-carbon options. This leading forum is where the power industry can meet, share and discuss solutions for India's energy future.";

}
