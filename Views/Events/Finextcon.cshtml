@{
    string domainUrl = "https://www.devdiscourse.com";
    ViewBag.Title = "FINEXT DUBAI CONFERENCE 2020 | Devdiscourse";
    ViewBag.webEdition = ViewBag.region;
    ViewBag.alternateHeadline = "FINEXT DUBAI CONFERENCE 2020";
    ViewBag.pageType = "webpage";
    string imageUrl = "/images/mediapartners/Finext-Conference-dubai-resize";
    ViewBag.PageImage = string.Concat(domainUrl, imageUrl);
    ViewBag.PostUrl = "https://www.devdiscourse.com/events/Finextcon";
    ViewBag.pageKeywords = "FINEXT DUBAI 2020, FiNext Awards & Conference provides a platform for the global FinTech sector, allowing start-ups, SMEs, incumbent tech providers, investors";
    ViewBag.PageDescription = "FiNext Awards & Conference provides a platform for the global FinTech sector, allowing start-ups, SMEs, incumbent tech providers, investors, financial institutions and other stakeholders to connect and interact under one roof. In addition to the conference, FiNext Tech Awards acknowledge the efforts of FinTech thought leaders, torchbearers of the industry.";

}
<style>
    .renderBody {
        margin: 0;
        background: #D96F41;
    }

    .fg-color {
        color: #133100;
    }

    .bg-color {
