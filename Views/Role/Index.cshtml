@model IEnumerable<IdentityRole>
@{
    ViewBag.Title = "Add Role";
    Layout = "~/Views/Shared/_Layout _plain.cshtml";
}
<link href="~/Content/admin.css" rel="stylesheet" />
<div class="container bg-white m-t-10 m-b-10 p-b-10 p-t-10">
    <ol class="breadcrumb no-margin">
        <li class="breadcrumb-item"><a href="@Url.Action("AdminView", "Admin")">Admin</a></li>
        <li class="breadcrumb-item"><a href="@Url.Action("Index", "Role")">Role</a></li>
        <li class="breadcrumb-item active"><a href="@Url.Action("Create", "Role")">Create New</a></li>
    </ol>
</div>
<div class="container bg-white p-20">
    <table class="table">
        <tr>
            <th>User Role</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Name)</td>
            </tr>
        }
    </table>
</div>





