    google.charts.load('current', { 'packages': ['corechart'] });
    google.charts.setOnLoadCallback(drawChart);

    function InitPhotoLayer() {
        photoLayer = L.photo.cluster().on('click', function (evt) {
            var photo = evt.layer.photo,
                template = '{headerContent}<div><div class="image-cover"><img src="{url}" onerror="this.parentElement.parentElement.style.display=' + "'none'" + '" class="popup-image"/></div><div class="fullscreenImage" data-url="{url}">[view in fullscreen]</div></div><div>{content}</div>';
            if (photo.audio) {
                template = '{headerContent}<audio controls><source src="{audio}" onerror="this.parentElement.style.display=' + "'none'" + '" type="audio/wav"></audio><div>{content}</div>';
            };
            if (photo.video && (!!document.createElement('video').canPlayType('video/mp4; codecs=avc1.42E01E,mp4a.40.2'))) {
                template = '{headerContent}<div class="video-cover"><video controls><source src="{video}" onerror="this.parentElement.parentElement.style.display=' + "'none'" + '" type="video/mp4"/></video></div><div>{content}</div>';
            };
            if (photo.url == null && photo.video == null && photo.audio == null) {
                template = '{headerContent}<div>{content}</div>';
            };
            evt.layer.bindPopup(L.Util.template(template, photo), {
                className: 'leaflet-popup-photo',
                minWidth: 250
            }).openPopup();
        });
        $(".markerloader").hide();
    }

    function drawChart() {
        var data = new google.visualization.DataTable();
        data.addColumn('string', 'Sector');
        data.addColumn('number', 'Feedbacks');

        $.getJSON(chartDataUrl, function (result) {
            $.each(result, function (i, item) {
                totalFeedbacks = totalFeedbacks + item.responseCount;
                data.addRow([item.title, item.responseCount]);
            });
            $('#totalFb').html(totalFeedbacks);
            var options = {
                title: 'Sector wise feedback',
                pieHole: 0.4
            };
            var chart = new google.visualization.PieChart(document.getElementById('summaryChart'));
            chart.draw(data, options);
        });
    }
    // Map Code Here ---------------------------------------------//
        var dataUrl = "https://ideame.io/Survey/GetLocationResponse?location=" + loc + "&page=" + markerPage;
    var map = {};
    var markerGroup = {};
    var photoLayer = {};

    if (sectors != "") {
        sectorArr = sectors.split(',');
    }

    $('.form').each(function () {
        var _fid = this.id;
        if (sectorArr.length > 0) {
            for (var i = 0; i < sectorArr.length; i++) {
                if (_fid == sectorArr[i]) {
                    $('#' + _fid).show();
                }
            }
        }
        else {
            $('#' + _fid).show();
        }
    });
    // By Default --------------------------------------//
    if (sectors != "") {
        dataUrl = "https://ideame.io/Survey/GetSurveyResponse?list=" + sectors + "&location=" + loc + "&page=" + markerPage;
        markerFor = "Sector";
    }
    // Map Code -----------------------------------------//
    //------ After Click on Filter
    $(".form").click(function () {
        $("#legendBox").hide();
        if (this.id == "All") {
            markerFor = "All";
            $(".form").removeClass('active');
            $('#' + this.id).addClass('active');
            markerPage = 1;
            dataUrl = "https://ideame.io/Survey/GetLocationResponse?location=" + loc + "&page=" + markerPage;
            if (sectors != "") {
                dataUrl = "https://ideame.io/Survey/GetSurveyResponse?list=" + sectors + "&location=" + loc + "&page=" + markerPage;
                markerFor = "Sector";
            }
            if (photoLayer != undefined) {
                map.removeLayer(photoLayer);
                InitPhotoLayer();
            };
            allowGetmarker = true;
            PhotoLayer();
        }
        else {
            getSdgQuestion("https://ideame.io/Survey/GetSdgQuestion/" + this.id);
        }
        _clickId = this.id;
    });
    function getSdgQuestion(url) {
        $.getJSON(url, function (data) {
            var html = '';
            $('#questionContent').html('');
            $.each(data, function (i, item) {
                var options = {};
                if (item.Type == 12) {
                    options = item.colOptions;
                } else {
                    options = item.rowOptions;
                }
                if (i == 0) {
                    html += '<div class="radio"><label><input type="radio" class="component" value="' + item.id + '" tabindex="' + options.join(",") + '" name="radio" checked><span style="margin-left:20px;">' + item.title + '</span></label></div>';
                }
                else {
                    html += '<div class="radio"><label><input type="radio" class="component" value="' + item.id + '" tabindex="' + options.join(",") + '" name="radio"><span style="margin-left:20px;">' + item.title + '</span></label></div>';
                }
            });
            $('#questionContent').html(html);
            $("#questionModel").modal("show");
        });
    }
    $('#okBtn').click(function () {
        markerFor = "Survey";
        $(".form").removeClass('active');
        $('#' + _clickId).addClass('active');

        idArray = [];
        var legend1 = [];
        var legend2 = [];
        var legend3 = [];
        var legend4 = [];
        var legend5 = [];
        var legend6 = [];
        $('.component').each(function () {
            if ($(this).is(":checked")) {
                idArray.push($(this).val());
                var options = $(this).attr("tabindex");
                var optionArr = options.split(",");
                optionArr.forEach(function (opt, index) {
                    switch (opt) {
                        case "Very good":
                        case "Very much":
                        case "Very proficient":
                        case "Free":
                        case "No losses":
                        case "Very much improved":
                        case "Clean":
                        case "Diabetes":
                        case "Malaria":
                        case "No road connectivity":
                        case "Substantially reduced":
                        case "Substantially decreased":
                            legend1.push(opt);
                            break;
                        case "Good":
                        case "Substantial":
                        case "Proficient":
                        case "Very cheap":
                        case "Insignificant losses":
                        case "Somewhat improved":
                        case "Slightly polluted":
                        case "Cancer":
                        case "Tuberculosis":
                        case "Accident blackspots":
                        case "Somewhat reduced":
                        case "Somewhat decreased":
                            legend2.push(opt);
                            break;
                        case "Average":
                        case "Intermediate":
                        case "Affordable":
                        case "Some losses":
                        case "Same as earlier":
                        case "Moderately polluted":
                        case "Cardiovascular disease":
                        case "Hepatitis B":
                        case "Unclean roads":
                            legend3.push(opt);
                            break;
                        case "Not so good":
                        case "Very less":
                        case "Costly":
                        case "Beginner":
                        case "Substantial losses":
                        case "Polluted":
                        case "Chronic respiratory disease":
                        case "HIV infections":
                        case "Congested roads":
                        case "Somewhat deteriorated":
                        case "Somewhat increased":
                            legend4.push(opt);
                            break;
                        case "Poor":
                        case "Never used":
                        case "Very costly":
                        case "Huge losses":
                        case "Poisonous":
                        case "Damaged roads":
                        case "Deteriorated":
                        case "Substantially increased":
                            legend5.push(opt);
                            break;
                        case "Not available":
                        case "Not at all":
                        case "Very much deteriorated":
                            legend6.push(opt);
                            break;
                    }
                });

                var unique1 = legend1.filter(function (optionArr, i, a) {
                    return i == a.indexOf(optionArr);
                });
                var unique2 = legend2.filter(function (optionArr, i, a) {
                    return i == a.indexOf(optionArr);
                });
                var unique3 = legend3.filter(function (optionArr, i, a) {
                    return i == a.indexOf(optionArr);
                });
                var unique4 = legend4.filter(function (optionArr, i, a) {
                    return i == a.indexOf(optionArr);
                });
                var unique5 = legend5.filter(function (optionArr, i, a) {
                    return i == a.indexOf(optionArr);
                });
                var unique6 = legend6.filter(function (optionArr, i, a) {
                    return i == a.indexOf(optionArr);
                });
                $('.li').show();
                $("#legend1").html(unique1.join(", "));
                $("#legend2").html(unique2.join(", "));
                $("#legend3").html(unique3.join(", "));
                $("#legend4").html(unique4.join(", "));
                $("#legend5").html(unique5.join(", "));
                $("#legend6").html(unique6.join(", "));
                if (unique1.length == 0) {
                    $("#li1").hide();
                }
                if (unique2.length == 0) {
                    $("#li2").hide();
                }
                if (unique3.length == 0) {
                    $("#li3").hide();
                }
                if (unique4.length == 0) {
                    $("#li4").hide();
                } if (unique5.length == 0) {
                    $("#li5").hide();
                }
                if (unique6.length == 0) {
                    $("#li6").hide();
                }
            }
        });
        dataUrl = "https://ideame.io/Survey/GetLocationResponse?list=" + idArray.join(',') + "&location=" + loc;
        $("#questionModel").modal("hide");
        $("#legendBox").show();
        if (photoLayer != undefined) {
            map.removeLayer(photoLayer);
            InitPhotoLayer();
        };
        allowGetmarker = true;
        PhotoLayer();
    });
    CreateMap();
    $(function () {
        // Reference the auto-generated proxy for the hub.
        var chat = $.connection.chatHub;
        // Create a function that the hub can call back to display messages.
        chat.client.updateDataonMap = function (recieveUrl) {
            dataUrl = recieveUrl;
            InitPhotoLayer();
            PhotoLayer();
        };
        $.connection.hub.start().done(function () {
            var Url = dataUrl;
            chat.server.updateMap(Url);
        });
    });
    function CreateMap() {
        map = L.map('map', {
            zoomControl: false, fullscreenControl: true, minZoom: 2,
            maxZoom: 20
        }).setView([29.00535, 77.00485], 2);
        L.tileLayer('http://{s}.google.com/vt/lyrs=m&x={x}&y={y}&z={z}', {
            maxZoom: 20,
            subdomains: ['mt0', 'mt1', 'mt2', 'mt3'],
            label: "Google Street"
        }).addTo(map);
        var zoomHome = L.Control.zoomHome();
        zoomHome.addTo(map);
    }

    function PhotoLayer() {
        $.getJSON(dataUrl, function (data) {
            if (data.length == 0) {
                $(".markerloader").hide();
                allowGetmarker = false;
                return;
            }
            if (data.length < 100) {
                allowGetmarker = false;
                $(".markerloader").hide();
            }
            var photos = [];
            $.each(data, function (i, item) {
                var locat = item.location.split(',');
                if (locat[0] != "" && locat[1] != "") {
                    var iconUrl = "";
                    switch (item.title) {
                        case "Education facilities":
                            iconUrl = '/AdminFiles/Monitoring/education.svg';
                            break;
                        case "Energy services":
                            iconUrl = '/AdminFiles/Monitoring/energy.svg';
                            break;
                        case "Environmental status":
                            iconUrl = '/AdminFiles/Monitoring/environment.svg';
                            break;
                        case "Gender equality and women empowerment":
                            iconUrl = '/AdminFiles/Monitoring/gender.svg'
                            break;
                        case "Governance":
                            iconUrl = '/AdminFiles/Monitoring/govt.svg';
                            break;
                        case "Public health status and facilities":
                            iconUrl = '/AdminFiles/Monitoring/health.svg';
                            break;
                        case "Status of Agriculture sector":
                            iconUrl = '/AdminFiles/Monitoring/agriculture.svg';
                            break;
                        case "Transport":
                            iconUrl = '/AdminFiles/Monitoring/transport.svg';
                            break;
                        case "Water and sanitation facilities":
                            iconUrl = '/AdminFiles/Monitoring/wash.svg';
                            break;
                    }

                    var bg = "";
                    var resLocation = "";
                    resLocation = item.locationTitle.replace(new RegExp(null, 'g'), "");
                    if (resLocation != " " && !resLocation.startsWith(',')) {
                        resLocation = ", " + resLocation;
                    }

                    var responseValue = item.value;
                    bg = getBgColor(responseValue);
                    if (item.iurl != "") { bg = bg + " active"; }
                    else if (item.vurl != "") { bg = bg + " active"; }
                    else if (item.aurl != "") { bg = bg + " active"; }
                    photos.push({
                        lat: locat[0],
                        lng: locat[1],
                        url: (item.iurl ? "https://ideame.io/images/surveyfile/" + item.iurl : null),
                        caption: item.question,
                        headerContent: '<h4><img src="' + item.imageUrl + '"  onerror="this.src=' + "'/AdminFiles/Logo/img_avatar.png'" +'" class="img-circle imgcss" /> ' + item.name + '<span style="font-size:12px">' + resLocation + '</span></h4>',
                        content: '<h4>' + item.question + '</h4><p>' + responseValue + '</p><hr style="margin:8px 0;"/><span class="fa fa-comments-o"></span><a href="/Research/SurveyResponse?id=' + item.id + '" class="fg-black text-decoration"> Comment</a><span class="reportTo" style="float:right;cursor:pointer" id="' + item.id + '" tabindex="' + item.question + '"><img src="/images/SDGs/report_flag.svg" style="width:13px;display:inline" /> Report</span>',
                        thumbnail: iconUrl,
                        iconBg: bg,
                        video: (item.vurl ? "https://ideame.io/images/surveyfile/" + item.vurl : null),
                        audio: (item.aurl ? "https://ideame.io/images/surveyfile/" + item.aurl : null),
                    });
                    function getBgColor(responseValue) {
                        var bg = "";
                        switch (responseValue) {
                            case "Very good":
                            case "Very much":
                            case "Very proficient":
                            case "Free":
                            case "No losses":
                            case "Very much improved":
                            case "Clean":
                            case "Diabetes":
                            case "Malaria":
                            case "No road connectivity":
                            case "Substantially reduced":
                            case "Substantially decreased":
                                bg = "bg1";
                                break;
                            case "Good":
                            case "Substantial":
                            case "Proficient":
                            case "Very cheap":
                            case "Insignificant losses":
                            case "Somewhat improved":
                            case "Slightly polluted":
                            case "Cancer":
                            case "Tuberculosis":
                            case "Accident blackspots":
                            case "Somewhat reduced":
                            case "Somewhat decreased":
                                bg = "bg2";
                                break;
                            case "Average":
                            case "Intermediate":
                            case "Affordable":
                            case "Some losses":
                            case "Same as earlier":
                            case "Moderately polluted":
                            case "Cardiovascular disease":
                            case "Hepatitis B":
                            case "Unclean roads":
                                bg = "bg3";
                                break;
                            case "Not so good":
                            case "Very less":
                            case "Costly":
                            case "Beginner":
                            case "Substantial losses":
                            case "Polluted":
                            case "Chronic respiratory disease":
                            case "HIV infections":
                            case "Congested roads":
                            case "Somewhat deteriorated":
                            case "Somewhat increased":
                                bg = "bg4";
                                break;
                            case "Poor":
                            case "Never used":
                            case "Very costly":
                            case "Huge losses":
                            case "Poisonous":
                            case "Damaged roads":
                            case "Deteriorated":
                            case "Substantially increased":
                                bg = "bg5";
                                break;
                            case "Not available":
                            case "Not at all":
                            case "Very much deteriorated":
                                bg = "bg6";
                                break;
                            default:
                                bg = "no-answer";
                                break;
                        }
                        return bg;
                    }
                }
            });
            photoLayer.add(photos).addTo(map);
            if (markerPage == 1) {
                map.fitBounds(photoLayer.getBounds());
            }
            markerPage = markerPage + 1;
            if (markerFor == "Sector") {
                 dataUrl = "https://ideame.io/Survey/GetSurveyResponse?list=" + sectors + "&location=" + loc + "&page=" + markerPage;
            } else if (markerFor=="All") {
                dataUrl = "https://ideame.io/Survey/GetLocationResponse?location=" + loc + "&page=" + markerPage;
            }
            else if (markerFor == "Survey") {
                dataUrl = "https://ideame.io/Survey/GetLocationResponse?list=" + idArray.join(',') + "&location=" + loc;
            }
            if (allowGetmarker == true) {
                PhotoLayer();
            }
        });
    }
    $(document).on("click", ".fullscreenImage", function () {
        var imgUrl = $(this).data("url");
        $("#map-image-container").html('<img src="' + imgUrl + '" />');
        $("#map-image-popup").show();

    });
    $("#close-map-image").click(function () {
        $("#map-image-popup").hide();
    });
    $(document).on('click', '.reportTo', function () {
        @{
            if (User.Identity.IsAuthenticated) {
                <text>
                    $('#reportModal').modal('show');
                    _resId = this.id;
                    _repQues = $(this).attr("tabindex");
                </text>
            }
            else {
                <text>
                    window.location.href = "/Account/Login?returnUrl=/Research/Dashboard";
                </text>
            }
        }
    });
    $('#submitReport').click(function () {
        $('#reportModal').modal('hide');
        _repText = $('#reportText').val();
        if (_repText == "") {
            alert("Please enter report text!");
        }
        else {
            responseReport();
        }
    });
    function responseReport() {
        $.ajax({
            url: '/Research/ResponseReport',
            type: 'Post',
            data: { id: _resId, text: _repText, question: _repQues },
            dataType: 'Json',
            success: function (data) {
                alert(data);
            },
            error: function () {
            },
        });
    }
    // Map Code End Here -----------------------------------------//
    </script>
}

