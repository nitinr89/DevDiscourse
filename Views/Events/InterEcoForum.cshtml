    }

    .event-news {
        height: 240px;
        background-repeat: no-repeat;
        background-size: cover
    }

    .eventcard-title {
        height: 90px;
        color: #222;
        background-color: #fff;
        font-stretch: condensed;
        font-size: 14px;
        padding: 10px;
    }

    .loadMoreButton {
        font-size: 16px;
        color: #222;
        background-color: #fff;
        display: block;
        line-height: 30px;
        text-align: center;
        cursor: pointer;
        padding: 10px;
        border: 1px solid #eee;
    }

    .event-container {
        padding-top: 140px;
        padding-bottom: 100px;
    }

    .divCss {
        color: #fff;
        background-color: #959D93;
        padding-bottom:10px;
        padding-top:10px;
    }

    .eventTitle {
        font-size:50px;
        color:#fff;
        font-weight:500;
    }

    .eventVenue {
