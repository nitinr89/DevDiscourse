            ViewBag.pageKeywords = "Devdiscourse News, International Development News, North America, United State, Devdiscourse Canada, SDGs News, NBA News, Politics News, Sports News, Technology News";
            break;
        case "Latin America and Caribbean":
            ViewBag.Title = "Latin America & Caribbean News - International Development News | Devdiscourse";
            ViewBag.alternateHeadline = "Latin America & Caribbean News - International Development News | Devdiscourse";
            ViewBag.PageDescription = "Devdiscourse: Get Latest Headline From Latin America & Caribbean. International Development News, Breaking News on Politics, Business, Technology, Science, Health, Education and Entertainment.";
            ViewBag.PostUrl = "https://www.devdiscourse.com/latin-america";
            ViewBag.pageKeywords = "Devdiscourse news, International Development News, Latin America news, Caribbean News, Brazil News, Argentina News, Cambodia News, Chile News, Peru News, Politics News, Business News, Sports News, Technology News";
            break;
    }
}
