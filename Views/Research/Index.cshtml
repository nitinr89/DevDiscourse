                            thumbnail: iconUrl,
                            tranparentBg: '/Content/transparent.png',
                            iconBg: bg,
                            video: (item.vurl ? "https://ideame.io/images/surveyfile/" + item.vurl : null),
                            audio: (item.aurl ? "https://ideame.io/images/surveyfile/" + item.aurl : null),
                        });
                        function getBgColor(responseValue) {
                            var bg = "";
                            switch (responseValue) {
                                case "Very good":
                                case "Very Good":
                                case "Very much":
                                case "Very proficient":
                                case "Free":
                                case "No losses":
