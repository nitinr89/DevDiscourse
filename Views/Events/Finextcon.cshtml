        background-image: url('/images/mediapartners/Finext-Conference-dubai-resize.jpg');
        background-size: cover;
        background-repeat: no-repeat;
        background-position: center;
        position:relative;
    }
    .gard-overlay{
        position:absolute;
        width:100%;
        height:100%;
