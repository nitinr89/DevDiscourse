    }

    nav.navscrollmenubar .linkBtn {
        display: inline-block;
        text-align: center;
        padding: 14px 17px;
        text-decoration: none;
        text-transform: uppercase;
        color: #fff;
        font-size: 18px;
        font-family: 'Oswald',sans-serif
    }

    .event-news {
        height: 240px;
        background-repeat: no-repeat;
        background-size: cover
    }

    .eventcard-title {
        height: 90px;
        color: #222;
        background-color: #fff;
        font-stretch: condensed;
        font-size: 14px;
        padding: 10px;
    }

    .loadMoreButton {
        font-size: 16px;
        color: #222;
        background-color: #fff;
        display: block;
        line-height: 30px;
        text-align: center;
        cursor: pointer;
        padding: 10px;
        border: 1px solid #eee;
    }

    .event-container {
        padding-top: 140px;
        padding-bottom: 160px;
    }

    .eventVenue {
        font-size: 20px;
        color: white;
        font-family: sans-serif;
    }

    .eventDesc {
        font-size: 50px;
        color: white;
        font-family: sans-serif;
        font-weight: bolder;
        letter-spacing: -1px;
        font-stretch:condensed;
    }

    .bg-white.p-t-20 {
